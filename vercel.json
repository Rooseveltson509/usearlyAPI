{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/tesseract/(.*)",
      "dest": "/public/tesseract/$1"
    }
  ],
  "functions": {
    "index.js": {
      "includeFiles": [
        "node_modules/tesseract.js-core/tesseract-core-simd.wasm"
      ],
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
